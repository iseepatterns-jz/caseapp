# Production Environment Configuration
# This file should be customized for your AWS deployment

# Application
APP_NAME=Court Case Management System
DEBUG=false
VERSION=1.0.0
ENVIRONMENT=production

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your-account-id-here

# Database (will be populated by CDK deployment)
DATABASE_URL=postgresql://username:password@rds-endpoint:5432/courtcase_db

# S3 Buckets (will be created by CDK)
S3_BUCKET_NAME=court-case-documents-prod
S3_MEDIA_BUCKET=court-case-media-prod

# Redis (will be created by CDK)
REDIS_URL=redis://elasticache-endpoint:6379

# OpenSearch (will be created by CDK)
OPENSEARCH_ENDPOINT=https://opensearch-endpoint.us-east-1.es.amazonaws.com

# Cognito (will be created by CDK)
COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx
COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
COGNITO_CLIENT_SECRET=xxxxxxxxxxxxxxxxxxxxxxxxxx

# Security
SECRET_KEY=your-super-secure-secret-key-here
MFA_REQUIRED=true
PASSWORD_MIN_LENGTH=12

# AI Services
ENABLE_AI_FEATURES=true
CASE_CATEGORIZATION_MODEL=amazon.titan-text-express-v1

# Compliance
HIPAA_COMPLIANCE=true
SOC2_COMPLIANCE=true
AUDIT_LOG_RETENTION_DAYS=2555

# Monitoring
LOG_LEVEL=INFO